{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
"noImplicitAny": false,
"esModuleInterop": true,
"jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
